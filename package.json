{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate to setup your react-redux app with eslint lint and jest",
  "author": "Nikita-Jajodia",
  "repository": "https://github.com/nikitajajodia/react-redux-boilerplate.git",
  "scripts": {
    "dev": "webpack-dev-server --historyApiFallback true --port 8888 --content-base build/",
    "test": "jest",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFiles": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "html-webpack-plugin": "3.2.0",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "save": "^2.3.3",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.6.0",
    "babel-loader": "7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^2.1.1",
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "1.2.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "react-test-renderer": "16.4.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
